{
  "name": "@wiretap/db",
  "version": "1.0.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch --preserveWatchOutput",
    "lint": "eslint .",
    "ts:check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:migration-generate": "tsx ./src/scripts/db-generate.ts",
    "db:migration-push": "tsx ./src/scripts/db-push.ts",
    "db:migration": "pnpm run db:migration-generate && pnpm run db:migration-push",
    "test-db:migration": "NODE_OPTIONS='--import tsx' drizzle-kit generate && drizzle-kit migrate --config drizzle.test.config.ts",
    "local-script:reindex-pools-addresses": "tsx ./src/scripts/reindex-pools-addresses.ts",
    "local-script:reindex-token-image-url": "tsx ./src/scripts/reindex-token-image-url.ts"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.0",
    "@wiretap/config": "workspace:*",
    "@wiretap/utils": "workspace:*",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.0",
    "drizzle-orm": "^0.42.0",
    "pg": "^8.14.1",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/pg": "^8.11.13",
    "@wiretap/eslint-config": "workspace:*",
    "@wiretap/prettier-config": "workspace:*",
    "@wiretap/typescript-config": "workspace:*",
    "vite": "^6.3.2",
    "vitest": "^3.1.2",
    "@uniswap/sdk-core": "^7.7.2",
    "@uniswap/v3-sdk": "^3.25.2"
  }
}
